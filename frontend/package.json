{
  "name": "trending",
  "version": "1.0.0",
  "main": "index.js",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.+(js|jsx|vue|ts|tsx|json|css|scss|md|graphql)\" && npm run lint ----fix",
    "start": "webpack-dev-server --mode=development --quiet --overlay",
    "lint": "eslint '{src,apps,libs,test}/**/*.+(ts|tsx)' --fix && tsc --p tsconfig.json --noEmit",
    "test": "jest",
    "test:cov": "jest --coverage",
    "build": "webpack --mode=production",
    "profile": "webpack --mode=production --profile",
    "api": "sta -p http://localhost:8000/api-json -o ./src --no-client"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
